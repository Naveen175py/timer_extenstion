{
    "manifest_version":3,
    "name": "Timer Extention",
    "version":"1.0.0",
    "description": "hello chrome world",
    "icons":{
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "action":{
        "default_icons":{
            "16": "icon.png",
            "24": "icon.png",
            "32": "icon.png"
        },
        "default_title": "Timer Extenson Action title",
        "default_popup": "popup.html"

    },
    "options_page":"options.html",
    "permissions":["storage","alarms","notifications","registration"],
    "background":{
        "service_worker": "background.js"
    }

}